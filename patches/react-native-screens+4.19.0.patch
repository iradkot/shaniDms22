+import type { NativeSyntheticEvent } from 'react-native';
+import { StackScreenProps } from './StackScreen.types';
+
+export const StackScreenLifecycleState = {
+  INITIAL: 0,
+  DETACHED: 1,
+  ATTACHED: 2,
+} as const;
+
+/**
+ * EXPERIMENTAL API, MIGHT CHANGE W/O ANY NOTICE
+ */
+function StackScreen({
+  children,
+  // Control
+  maxLifecycleState,
+  screenKey,
+  // Events
