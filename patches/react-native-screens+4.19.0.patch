+                                             variant:nil
+                                     scaleMultiplier:1.0];
+  } else {
+    CGFloat resolvedFontSize = fontSize ? [fontSize floatValue] : 0;
+    if (resolvedFontSize == 0) {
+#if TARGET_OS_TV
+      resolvedFontSize = 17.0;
+#else
+      resolvedFontSize = [UIFont labelFontSize];
+#endif
+    }
